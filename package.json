{
  "name": "stellar-base",
  "version": "0.10.0",
  "description": "Low level stellar support library",
  "main": "lib/index.js",
  "scripts": {
    "test": "babel-node ./node_modules/gulp/bin/gulp.js test:node",
    "docs": "./node_modules/.bin/jsdoc ./src/ -d docs/",
    "preversion": "gulp test",
    "version": "gulp build",
    "postversion": "git push && git push --tags",
    "prettier-all": "prettier --write **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browser": {
    "ed25519": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stellar/js-stellar-base.git"
  },
  "keywords": [
    "stellar"
  ],
  "engines": {
    "node": ">=6.14.0"
  },
  "author": "Scott Fleckenstein <scott@stellar.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stellar/js-stellar-base/issues"
  },
  "homepage": "https://github.com/stellar/js-stellar-base",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "~6.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^2.2.0",
    "clear": "^0.1.0",
    "coveralls": "^2.11.4",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prefer-import": "^0.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.9.8",
    "ghooks": "^0.3.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.3",
    "gulp-coveralls": "^0.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-git": "~0.5.3",
    "gulp-istanbul": "^1.1.1",
    "gulp-load-plugins": "^0.9.0",
    "gulp-mocha": "^2.0.1",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "~1.2.0",
    "gulp-rimraf": "~0.1.0",
    "gulp-uglify": "~0.3.1",
    "gulp-webpack": "^1.3.1",
    "husky": "^1.3.1",
    "isparta": "4.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "3.5.5",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.1.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon-chai": "^0.3.0",
    "karma-webpack": "^1.8.0",
    "lint-staged": "7.3.0",
    "minami": "^1.1.1",
    "mocha": "~1.17.1",
    "prettier": "^1.16.1",
    "run-sequence": "^1.0.2",
